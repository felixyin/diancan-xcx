/* pages/yuyuezhuowei/jieguo/jieguo.wxss */
page {
  background: #fff;
}
.paidui_shop {
  color: #ed6a0c;
  background-color: #fffbe8;
  padding: 20rpx;
  font-size: 32rpx;
  font-weight: bold;
  margin: 20rpx;
  border-radius: 10rpx;
}
.jieguo_body {
  text-align: center;
  margin-top: 40rpx;
  position: absolute;
  top: 40%;
  left: 50%;
  transform: translate(-50%,-50%);
  width: 80%;
}
.jieguo_body .text {
  font-size: 28rpx;
  font-weight: bold;
  text-align: center;
}
.jieguo_body .number {
  padding: 40rpx 0 20rpx;
  line-height: 1;
  position: relative;
  display: flex;
  align-items: center;
  justify-content: center;
}

.jieguo_body .number .number_body {
  position: relative;
  line-height: 1;
}
.number_body_text {
  font-size: 100rpx;
  font-weight: bold;
}
.jieguo_body .number .tag_wrap {
  position: absolute;
  top: -15px;
  right: -40px;
  line-height: 1;
  display: block;
}

.jieguo_body .tip {
  font-size: 24rpx;
  color: #222;
}
.jieguo_area {
  margin:40rpx 20rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.wenxin_tip {
  background: #f0f0f0;
  color: #222;
  font-size: 24rpx;
  padding: 20rpx;
  border-radius: 10rpx;
  margin-top: 30rpx;
}



